{
  "compileOnSave": false,
  "compilerOptions": {
    "importHelpers": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "dom",
      "es2015"
    ],
    "module": "commonjs",
    "baseUrl": "./",
    "downlevelIteration": true
  },
  "files": [
    "main.ts"
  ],
  "include": [
    "*.test.ts"
  ]
}